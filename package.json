{
  "name": "birthdaypicker",
  "version": "0.1.18",
  "description": "i18n birthday picker",
  "browser": "dist/birthdaypicker.esm.min.js",
  "author": "wolfgang jungmayer",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint 'src/**/*.js' || true",
    "prettify": "prettier --write 'src/**/*.js'",
    "dev": "webpack --mode development --watch --config config/webpack.dev.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "updatedemo": "cross-env NODE_ENV=production webpack --config config/webpack.docs.js",
    "browserslistupdate": "npx update-browserslist-db@latest"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "LICENSE",
    "index.js",
    "README.md"
  ],
  "keywords": [
    "birthdaypicker, datepicker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon3/birthdaypicker.git"
  },
  "homepage": "https://lemon3.github.io/birthdaypicker",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/preset-env": "^7.24.3",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  }
}
